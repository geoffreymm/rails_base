<h1>Admin::Base#index</h1>
<p>Find me in app/views/admin/base/index.html.erb</p>

<div id="user_nav">
  <% if current_user %>
    Logged in as <%= current_user.email %>.
    <%= link_to "Log out", admin_log_out_path %>
  <% else %>
    <%= link_to "Sign up", admin_sign_up_path %> or
    <%= link_to "log in", admin_log_in_path %>
  <% end %>
</div>

<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>
